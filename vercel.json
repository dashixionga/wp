{
    "version": 2,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.3.6"
        }
    },
    "routes": [
      { 
        "src": "/(css|js|img)/(.*)$",
        "dest": "/api/assets.php?type=$1&file=$2"
      },
      { 
        "src": "/(.*)",  
        "dest": "api/index.php" 
      }
    ]
}
